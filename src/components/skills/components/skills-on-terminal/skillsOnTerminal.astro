---
import TerminalIcon from '../../atoms/terminal-icon/TerminalIcon.astro';
import Terminal from '../terminal/Terminal.astro';
import './skills-on-terminal.css';
---

<Terminal nombre="SLynch">
	<div class="terminal__skills">
		<h2>My Skills</h2>
		<div class="terminal__skills__container">
			<TerminalIcon
				src="/Portfolio/assets/mysql.svg"
				color="#7738ff"
				name="SQL"
			/>
			<TerminalIcon
				src="/Portfolio/assets/angular.svg"
				color="#FF4747"
				name="Angular"
			/>
			<TerminalIcon
				src="/Portfolio/assets/react.svg"
				color="#00D1FF"
				name="React"
			/>
			<TerminalIcon src="/Portfolio/assets/java.svg" color="#F3CBC2" name="Java" />
			<TerminalIcon src="/Portfolio/assets/fsharp.svg" color="#00A3FF" name="C#/F#" />
			<TerminalIcon
				src="/Portfolio/assets/flutter.svg"
				color="#3F75FF"
				name="Flutter"
			/>
		</div>
	</div>
</Terminal>
